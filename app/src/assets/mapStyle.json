{
    "version": 8,
    "id": "ixmaps-style-by-spatialnasir@gmail.com",
    "name": "ixmaps-style",
    "projection": {
        "type": "mercator"
    },
    "glyphs": "http://localhost:5173/map-fonts/{fontstack}/{range}.pbf",
    "sources": {
        "ix-tiles": {
            "type": "vector",
            "url": "pmtiles://http://localhost:5173/hello/ixmaps.pmtiles"
        }
    },
    "layers": [
        {
            "id": "background",
            "source": "ix-tiles",
            "source-layer": "background",
            "type": "fill",
            "paint": {
                "fill-color": [
                    "get",
                    "fill"
                ]
            }
        },
        {
            "id": "icecaps",
            "source": "ix-tiles",
            "source-layer": "icecaps",
            "type": "fill",
            "paint": {
                "fill-color": [
                    "get",
                    "fill"
                ]
            }
        },
        {
            "id": "altitudes",
            "source": "ix-tiles",
            "source-layer": "altitudes",
            "type": "fill",
            "paint": {
                "fill-color": [
                    "get",
                    "fill"
                ]
            }
        },
        {
            "id": "climate",
            "source": "ix-tiles",
            "source-layer": "climate",
            "type": "fill",
            "paint": {
                "fill-color": [
                    "get",
                    "fill"
                ]
            }
        },
        {
            "id": "lakes",
            "source": "ix-tiles",
            "source-layer": "lakes",
            "type": "fill",
            "paint": {
                "fill-color": [
                    "get",
                    "fill"
                ]
            }
        },
        {
            "id": "political",
            "source": "ix-tiles",
            "source-layer": "political",
            "type": "fill",
            "paint": {
                "fill-color": [
                    "get",
                    "fill"
                ]
            }
        },
        {
            "id": "rivers",
            "source": "ix-tiles",
            "source-layer": "rivers",
            "type": "line",
            "paint": {
                "line-color": [
                    "get",
                    "fill"
                ],
                "line-width": 1
            }
        },
        {
            "id": "political-names",
            "source": "ix-tiles",
            "source-layer": "political",
            "type": "symbol",
            "layout": {
                "symbol-placement": "point",
                "text-field": [
                    "get",
                    "id"
                ],
                "text-font": [
                    "Barlow Regular"
                ],
                "text-size": 16
            }
        }
    ]
}